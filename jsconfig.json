{
    "compilerOptions": {
      "jsx": "react",
      "target": "es2017",
      "allowSyntheticDefaultImports": false,
      "baseUrl": "./",
      "paths": {
        // "Config/*": ["src/config/*"],
        "@Components/*": ["src/components/*"],
        "@Assets/*": ["src/assets/*"],
